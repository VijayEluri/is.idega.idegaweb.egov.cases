<?xml version="1.0" encoding="UTF-8"?>

<process-definition
  xmlns=""  name="nestSomeProcess">
   <start-state name="start">
      <transition name="" to="submitNewCase"></transition>
   </start-state>
   <task-node name="submitNewCase">
      <transition name="" to="sendNewCaseMessages"></transition>
   </task-node>
   <node name="sendNewCaseMessages">
      <transition name="" to="caseOverview"></transition>
   </node>
   <task-node name="caseOverview">
      <transition name="moreInfoNeeded" to="moreInfoNeeded"></transition>
      <transition name="addComment" to="addComment"></transition>
      <transition name="inviteDefendant" to="inviteDefendant"></transition>
      <transition name="rule" to="submitRuling"></transition>
   </task-node>
   <fork name="moreInfoNeeded">
      <transition name="moreInfoNeeded" to="submitMessageToPlaintiff"></transition>
      <transition name="back" to="caseOverview"></transition>
   </fork>
   <task-node name="submitMessageToPlaintiff">
      <transition name="" to="submitAdditionalInformation"></transition>
   </task-node>
   <task-node name="submitAdditionalInformation">
      <transition name="" to="additionalInformationEnd"></transition>
   </task-node>
   <end-state name="additionalInformationEnd"></end-state>
   <task-node name="submitComment">
      <transition name="toCaseOverview" to="caseOverview"></transition>
   </task-node>
   <fork name="addComment">
      <transition name="addComment" to="submitComment"></transition>
      <transition name="back" to="caseOverview"></transition>
   </fork>
   <fork name="inviteDefendant">
      <transition name="sendEmail" to="sendEmail"></transition>
      <transition name="back" to="caseOverview"></transition>
   </fork>
   <node name="sendEmail">
      <transition name="" to="submitCaseAnswer"></transition>
   </node>
   <task-node name="submitCaseAnswer">
      <transition name="" to="caseAnswerEnd"></transition>
   </task-node>
   <end-state name="caseAnswerEnd"></end-state>
   <task-node name="submitRuling">
      <transition name="" to="sendRulingMessages"></transition>
   </task-node>
   <node name="sendRulingMessages">
      <transition name="" to="processEnd"></transition>
   </node>
   <end-state name="processEnd"></end-state>
</process-definition>